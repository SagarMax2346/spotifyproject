{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "spotifydatafactorysagar"
		},
		"SpotifyAzureSqlDatabase_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SpotifyAzureSqlDatabase'"
		},
		"SpotifyAzurewrite_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'SpotifyAzurewrite'"
		},
		"SpotifyAzureSqlDatabase_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "spotifyprojectserversagar.database.windows.net"
		},
		"SpotifyAzureSqlDatabase_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "spotifyprojectdbsagar"
		},
		"SpotifyAzureSqlDatabase_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminsagar"
		},
		"SpotifyAzurewrite_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://spotifydatalakesagar.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "spotifydatafactorysagar_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SpotifyAzureSqlDatabase_password": {
						"value": "[parameters('SpotifyAzureSqlDatabase_password')]"
					},
					"SpotifyAzurewrite_accountKey": {
						"value": "[parameters('SpotifyAzurewrite_accountKey')]"
					},
					"SpotifyAzureSqlDatabase_properties_typeProperties_server": {
						"value": "[parameters('SpotifyAzureSqlDatabase_properties_typeProperties_server')]"
					},
					"SpotifyAzureSqlDatabase_properties_typeProperties_database": {
						"value": "[parameters('SpotifyAzureSqlDatabase_properties_typeProperties_database')]"
					},
					"SpotifyAzureSqlDatabase_properties_typeProperties_userName": {
						"value": "[parameters('SpotifyAzureSqlDatabase_properties_typeProperties_userName')]"
					},
					"SpotifyAzurewrite_properties_typeProperties_url": {
						"value": "[parameters('SpotifyAzurewrite_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}